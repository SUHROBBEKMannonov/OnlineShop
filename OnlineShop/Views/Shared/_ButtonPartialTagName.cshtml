@model int
<td style=" width:150px">
	<div class="btn-group">
			<a asp-action="Edit" asp-controller="TagNames" class="btn btn-primary" asp-route-id="@Model"><i class="bi bi-pencil-square"></i></a>
			<a asp-action="Details" asp-controller="TagNames" class="btn btn-success" asp-route-id="@Model"><i class="bi bi-card-list"></i></a>
			<a asp-action="Delete" asp-controller="TagNames" class="btn btn-danger" asp-route-id="@Model"><i class="bi bi-trash"></i></a>
	</div>
</td>